{
  "name": "typescene",
  "version": "3.0.0-beta1",
  "description": "Typescene framework",
  "author": "Jelmer Cormont",
  "license": "MIT",
  "publishConfig": {
    "tag": "beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typescene/typescene.git"
  },
  "bugs": {
    "url": "https://github.com/typescene/typescene/issues"
  },
  "homepage": "http://typescene.dev/",
  "main": "./index.js",
  "module": "./index.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "JSX/dist",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf dist && rimraf JSX/dist",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && tsc -p src --declaration",
    "build": "tsc -p src --removeComments",
    "postbuild": "node test/run",
    "watch": "tsc -p src -w",
    "test": "node test/run"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "esm": "^3.2.25",
    "rimraf": "^3.0.2",
    "typescript": "3.9.7"
  }
}
