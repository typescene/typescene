{
  "compilerOptions": {
    "target": "ES5",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": ["es5", "dom", "es2015.promise", "es2015.iterable"],
    "outDir": "../dist",
    "downlevelIteration": true,
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "stripInternal": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
